# IQ Reseller Configuration
# Get your API key from IQ Reseller Admin Portal
IQR_API_KEY=your_iqr_api_key_here
IQR_AUTH_URL=https://signin.iqreseller.com
IQR_API_BASE_URL=https://api.iqreseller.com

# ShipStation Configuration
# Get your API key from ShipStation Settings > API Settings
SHIPSTATION_API_KEY=your_shipstation_api_key_here
SHIPSTATION_API_SECRET=your_shipstation_api_secret_here
SHIPSTATION_API_BASE_URL=https://ssapi.shipstation.com
SHIPSTATION_WEBHOOK_SECRET=your_webhook_secret_here
# ShipStation Store/Channel to send orders to (exact name as shown in ShipStation)
SHIPSTATION_STORE_NAME=DPC - Agent Quickbooks

# Sync Configuration
# How often to sync orders (in minutes). Set to 0 to disable scheduled sync.
SYNC_INTERVAL_MINUTES=15
# Number of orders to process in each batch
SYNC_BATCH_SIZE=50
# Maximum retry attempts for failed operations
SYNC_MAX_RETRIES=3

# Server Configuration
PORT=3000
NODE_ENV=development

